{
  "name": "seed-protocol-feed",
  "version": "0.0.1",
  "description": "Generate RSS, Atom, and JSON feeds from Seed Protocol GraphQL queries with dynamic routing",
  "keywords": [
    "seed-protocol",
    "rss",
    "atom",
    "json",
    "feed"
  ],
  "author": "Seed Protocol <hello@seedprotocol.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JournoLabs/seed-protocol-feed.git"
  },
  "homepage": "https://github.com/JournoLabs/seed-protocol-feed#readme",
  "bugs": {
    "url": "https://github.com/JournoLabs/seed-protocol-feed/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "seed-protocol-feed": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:client": "vite build",
    "dev:vite": "vite",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@seedprotocol/sdk": "0.4.2",
    "express": "^5.2.1",
    "feedsmith": "^2.9.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "@types/pluralize": "^0.0.33",
    "eslint": "^9.39.2",
    "rimraf": "^6.0.0",
    "typescript": "^5.5.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
